{
  "dashboard": {
    "id": null,
    "title": "FinTech Reconciliation System",
    "tags": ["fintech", "reconciliation", "devops"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Reconciliation Runs",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(reconciliation_runs_total[5m]))",
            "legendFormat": "Runs per second"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(reconciliation_runs_total{status=\"success\"}[5m])) / sum(rate(reconciliation_runs_total[5m])) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Total Discrepancy Amount",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(discrepancy_amount_total)",
            "legendFormat": "Total USD"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10000},
                {"color": "red", "value": 50000}
              ]
            },
            "unit": "currencyUSD"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Missing Transactions",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(missing_transactions_total)",
            "legendFormat": "Total Missing"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 500}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Reconciliation Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(reconciliation_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(reconciliation_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(reconciliation_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {"label": "Duration (seconds)", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(api_requests_total[5m])) by (processor)",
            "legendFormat": "{{processor}}"
          }
        ],
        "yAxes": [
          {"label": "Requests per second", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Database Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(database_operations_total{status=\"success\"}[5m])) by (operation)",
            "legendFormat": "{{operation}} (success)"
          },
          {
            "expr": "sum(rate(database_operations_total{status=\"error\"}[5m])) by (operation)",
            "legendFormat": "{{operation}} (error)"
          }
        ],
        "yAxes": [
          {"label": "Operations per second", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "System Resources",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_usage_bytes",
            "legendFormat": "Memory Usage"
          },
          {
            "expr": "active_database_connections",
            "legendFormat": "DB Connections"
          }
        ],
        "yAxes": [
          {"label": "Bytes / Connections", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
}